<form class="form-horizontal" method="post">
    <fieldset>
        <legend>用户<%= title %></legend>
        <div class="control-group">
            <label class="control-label" for="username">用户名</label>
            <div class="controls">
                <input type="text" class="input-xlarge" id="username" name="username" placeholder="请输入您的账户名称">
                <span id="username_span"></span>
                <p class="help-block">你的账户名称，用于登录和显示。</p>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="password">口令</label>
            <div class="controls">
                <input type="password" class="input-xlarge" id="password" name="password" placeholder="请输入您的账户密码">
                <span id="password_span"></span>
            </div>
        </div>
        <div class="form-actions">
            <button type="submit" class="btn btn-primary" onclick="return loginCheck();">登录</button>
        </div>
    </fieldset>
</form>
<script language="JavaScript" src="/javascripts/jquery.js"></script>
<script language="JavaScript" src="/javascripts/login.js"></script>
